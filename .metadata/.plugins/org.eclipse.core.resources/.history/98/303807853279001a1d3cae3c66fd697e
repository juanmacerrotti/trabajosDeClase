#include <stdio.h>
#include <stdlib.h>

int dividir (int operador1, int operador2 , float *pResultado)
{
	float resultado;
	int retorno=-1;

	if(pResultado != NULL && operador2 != 0)
	{
		resultado= (float)operador1/operador2;
		*pResultado=resultado;
		retorno= 0;
	}
	return retorno;
}

